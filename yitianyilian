#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,j;
    for(i=0;i<8;i++)
    {
                for(j=0;j<8;j++)
                if((j+i)%2==0)printf("%c",219);
                else printf(" ");
                
                printf("\n");
                
    }
    
    system("pause");
    return 0;}
